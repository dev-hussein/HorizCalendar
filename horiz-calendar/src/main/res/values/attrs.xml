<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HorizontalCalendar">
        <attr name="hc_day" format="integer" />
        <attr name="hc_year" format="integer" />
        <attr name="hc_month" format="enum">
            <enum name="january" value="0" />
            <enum name="february" value="1" />
            <enum name="march" value="2" />
            <enum name="april" value="3" />
            <enum name="may" value="4" />
            <enum name="june" value="5" />
            <enum name="july" value="6" />
            <enum name="august" value="7" />
            <enum name="september" value="8" />
            <enum name="october" value="9" />
            <enum name="november" value="10" />
            <enum name="december" value="11" />
        </attr>

        <attr name="hc_text_color" format="color" />
        <attr name="hc_error_text_color" format="color" />
        <attr name="hc_background_color" format="color" />
    </declare-styleable>
</resources>